include <iostream>
using namespace std;
class student
{
    public:
    string name;
    int age,roll_number;
    string grade;
};
int main()
{
    student s1;
    s1.name="Anku";
    s1.age=18;
    s1.roll_number=45;
    s1.grade="B+";
    cout<<s1.age<<"";
    
    student s2;
    s2.name="Rohit";
    s2.age=16;
    s2.roll_number=67;
    s2.grade="A+";
    cout<<s2.age<<"";
    
    
    
    
}

